{
    "name": "wakitsu",
    "version": "5.4.1",
    "description": "Hobby project for managing anime watch list on Kitsu through CLI",
    "type": "module",
    "main": "bin/main.js",
    "bin": {
        "wak": "./bin/main.js"
    },
